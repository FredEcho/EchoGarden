    "esbuild": "^0.25.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.1",
    "typescript": "5.6.3",
    "vite": "^7.1.4"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
